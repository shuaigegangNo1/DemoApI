<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sgg.rest.dao.AccountDao">

    <sql id="tableName">
    account
    </sql>


    <!-- 查询全部记录 -->
    <select id="selectAccountList" resultType="com.sgg.rest.model.Account">
        SELECT  * FROM account where 1 = 1
    </select>

    <select id="selectPageExt" resultType="com.sgg.rest.model.Account">
        SELECT  * FROM account where 1 = 1
    </select>
    <select id="selectAccountPage" resultType="com.sgg.rest.model.Account">
        SELECT  * FROM account where 1 = 1
    </select>
    
     <select id="selectPage" resultType="com.sgg.rest.model.Account">
        SELECT  * FROM account a where 1 = 1 order by a.id desc 
    </select>
</mapper>
